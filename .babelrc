{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-3",
        "react"
    ],
    "plugins": [
        "lodash",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "syntax-dynamic-import",
        "react-hot-loader/babel",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ],
}
